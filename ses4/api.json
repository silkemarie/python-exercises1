	});

	jQuery(document).ready(function($) {
		var consent = 0;
		var date = new Date();
		date.setTime(date.getTime() + 90 * 24 * 60 * 60 * 1000);
		var cookieDomain = '.kea.dk';
		$('.cookie-box button.btn.accept').click(function(event){
			event.preventDefault();
			var consent = 7;
			$.cookie('cookieConsent', consent, { expires: date, path: '/', domain: cookieDomain });
			// $.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
			$('.cookie-notice').fadeOut();
			$('.youtube-cookies').fadeOut();
			$('.embed-container').each(function() {
				var youtubeSrc = $(this).data('src');
				$(this).find('iframe').attr('src', youtubeSrc);
			});
		});
		$('.cookie-box button.btn.acceptselected').click(function(event){
			var consent = 0;
			$('.cookie-box label.cookie-selection').each(function() {
				consent += Number($(this).data('consent'));
			});
			event.preventDefault();
			$.cookie('cookieConsent', consent, { expires: date, path: '/', domain: cookieDomain });
			// $.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
			$('.cookie-notice').fadeOut();
			if (consent >= 4) {
				$('.youtube-cookies').fadeOut();
				$('.embed-container').each(function() {
					var youtubeSrc = $(this).data('src');
					$(this).find('iframe').attr('src', youtubeSrc);
				});
			}
		});
		$('.cookie-box button.btn.decline').click(function(event){
			event.preventDefault();
			$.cookie('cookieConsent', '0', { expires: date, path: '/', domain: cookieDomain });
			// $.cookie('cookieConsentId', null, { expires: -1, path: '/', domain: cookieDomain });
			$('.cookie-notice').fadeOut();
		});
		$('.cookie-box label.cookie-selection').click(function(event){
			$(this).find('.checkbox i').toggleClass('kea-icon-check-mark');
			if ($(this).find('.checkbox i').hasClass('kea-icon-check-mark')) {
				$(this).find('.checkbox').attr('aria-checked', 'true');
			} else {
				$(this).find('.checkbox').attr('aria-checked', 'false');
			};
			if ($(this).data('consent') == $(this).data('consentvalue')) {
				$(this).attr('data-consent', '0').data('consent', '0');
			} else {
				$(this).attr('data-consent', $(this).data('consentvalue')).data('consent', $(this).data('consentvalue'));
			}
			if ($('.cookie-box label.functional-cookies').data('consent') > 0 || $('.cookie-box label.statistics-cookies').data('consent') > 0 || $('.cookie-box label.marketing-cookies').data('consent') > 0) {
				$('.cookie-box .btn.accept').hide();
				$('.cookie-box .btn.acceptselected').show();
			} else {
				$('.cookie-box .btn.accept').show();
				$('.cookie-box .btn.acceptselected').hide();
			}
			var consent = 0;
			$('.cookie-box label.cookie-selection').each(function() {
				consent += Number($(this).data('consent'));
			});
		});
	});
	
