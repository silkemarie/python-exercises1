	var cookieDomain = '.kea.dk';
	jQuery(document).ready(function($) {
		$('.accept-functional-cookies').on('click', function () {
			var date = new Date();
			date.setTime(date.getTime() + 90 * 24 * 60 * 60 * 1000);
			var consent = parseInt($.cookie('cookieConsent')) + 4;
